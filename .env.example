# DEBUG = bool(strtobool(os.getenv('DEBUG', 'True')))
# FLASK_MAINTENANCE=
# JSONIFY_MAINTENANCE=

# Will be used as User-Agent value for API calls
JSONIFY_USER_AGENT='jsonify-bot'

# Base URL for airSlate API.
API_BASE_URI=https://api.airslate.com

# Bot's credentials obtained in https://developers.airslate.com .
BOT_CLIENT_ID=
BOT_CLIENT_SECRET=
